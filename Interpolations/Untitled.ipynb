   "source": [
    "def lagrange(nodes, values, x):\n",
    "    result = 0\n",
    "\n",
    "    for i in range(len(nodes)):\n",
    "        term = values[i]\n",
    "        for j in range(len(nodes)):\n",
    "            if i != j:\n",
    "                term *= (x - nodes[j]) / (nodes[i] - nodes[j])\n",
    "        result += term\n",
    "    return result\n"
   ]
  }
 ],
